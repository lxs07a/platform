<section class="page-title text-dark gradient-bg section-skewed big-padding-bt" data-gradients="#374346,#61c4da">
  <div class="row-parallax-bg">
    <div class="parallax-wrapper">
      <div class="parallax-bg">
        <img src="../../uploads/{{profilepic}}" alt="">
      </div>
    </div>
    <div class="parallax-overlay"></div>
  </div>
  <div class="centrize">
    <div class="v-center">
      <div class="container">
        <div class="row">
          <div class="col-md-8 col-md-offset-2">
            <div class="title text-center">
              <h1>{{nickname}}</h1>
              <h3>{{country}}</h3>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section class="section-skewed pt-50" data-custom-bg="#ffffff">
  <div class="container">
    <div class="row">
      <div class="col-md-7 mb-50">
        <div class="section-content">
          <h2 class="Pacifico">About Me</h2>
          <div>
            <p class="big">Full Name: <b>{{firstname}} {{lastname}}</b></p>
            <p class="big">Age: <b>{{age}}</b></p>
            <p class="big">Country of Origin: <b>{{country2}}</b></p>
            <p class="big">Profession: <b>{{profession}}</b></p>
            {{!-- <p class="big">Languages: <b>{{languages}}</b></p> --}}
          </div>
          <br><br><br>
          <p><b>Why I want to join UpCharge</b></p>
          <p class="big">{{question1}}</p>   
          <p><b>One thing I would like to change in the world</b></p>
          <p class="big">{{question2}}</p>
          <p><b>The superpower I'd like to have</b></p>
          <p class="big">{{question3}}</p> 
          <br><br><br> 
          <strong>Follow me on </strong><a href={{facebook_profile}} target="_blank"><u>Facebook</u></a>, 
          </strong><a href={{instagram_profile}} target="_blank"><u>Instagram</u></a>, 
          </strong><a href={{linked_in_profile}} target="_blank"><u>LinkedIn</u></a>, 
          </strong><a href={{twitter_profile}} target="_blank"><u>Twitter</u></a>    
        </div>
      </div>
      <div class="col-md-3 col-md-offset-2">
        <div class="project-info">
          <ul>
            <li><strong>Start date</strong>{{startdate}}</li>   
            <li><strong>End date</strong>{{enddate}}</li>
            <p><a class="btn" href="/users/single/{{id}}/message" data-scroll="true">Contact Me</a>
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>

